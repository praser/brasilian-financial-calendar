					"const moment = require(\"moment\");",
					"const firstDate = new Date(2001, 0, 1);",
					"const lastDate = new Date(2078, 8, 31);",
					"const format = \"YYYY-MM-DD\";",
					"const envStartDate = \"startDate\";",
					"const envEndDate = \"endDate\";",
					"const amountOfDays = parseInt(Math.random() * 100);",
					"",
					"pm.environment.unset(envStartDate);",
					"pm.environment.unset(envEndDate);",
					"",
					"const getRandomDate = (start, end) => {",
					"  return new Date(start.getTime() + Math.random() * (end.getTime() - start.getTime()));",
					"};",
					"",
					"const randomDate = moment(getRandomDate(firstDate, lastDate));",
					"const startDate = randomDate.format(format);",
					"const endDate = randomDate.add(amountOfDays, \"days\").format(format);",
					"",
					"pm.environment.set(envStartDate, startDate);",
					"pm.environment.set(envEndDate, endDate);",
					""
				]
			}
